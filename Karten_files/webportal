  overflow: visible;
  white-space: normal;
  -ms-word-wrap: break-word;
  word-wrap: break-word;
}
.text-with-line-breaks {
  display: block;
  line-height: 16px;
  /* wrap long text and urls */
  white-space: pre;
  /* CSS 2.0 */
  white-space: pre-wrap;
  /* CSS 3.0 */
  white-space: -pre-wrap;
  /* Opera 4-6 */
  white-space: -o-pre-wrap;
  /* Opera 7 */
  white-space: -moz-pre-wrap;
  /* Mozilla */
  -ms-word-wrap: break-word;
  word-wrap: break-word;
  /* IE 5+ */
}
